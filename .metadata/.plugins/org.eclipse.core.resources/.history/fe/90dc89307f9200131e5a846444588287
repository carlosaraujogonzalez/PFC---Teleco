package area.guias.pfc;

import android.graphics.Bitmap;

public class Aplicacion {
	// VARIABLES
			public int id;
			public String name;
			public String element_image_file_name;
			public String description;
			public String Url;
			public Bitmap image;
			
			// CONSTRUCTOR
			public Aplicacion(){
				this.id = -1;
				this.name = null;
				this.element_image_file_name = null;
				this.description = null;	
				this.Url = null;
				this.image = null;
			}
}
